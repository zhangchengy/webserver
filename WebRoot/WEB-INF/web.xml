<?xml version="1.0" encoding="UTF-8"?>
<web-app>

	<servlet>
		<servlet-name>LoginAction</servlet-name>
		<servlet-class>com.succez.login.java.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginAction</servlet-name>
		<url-pattern>/User/LoginAction</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>HelloWorld</servlet-name>
		<servlet-class>com.succez.login.java.HelloWorld</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloWorld</servlet-name>
		<url-pattern>/HelloWorld</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ListAction</servlet-name>
		<servlet-class>com.succez.login.java.ListAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListAction</servlet-name>
		<url-pattern>/User/ListAction</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>FileDownload</servlet-name>
		<servlet-class>com.succez.login.java.FileDownload</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FileDownload</servlet-name>
		<url-pattern>/User/FileDownload</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>list.html</welcome-file>
	</welcome-file-list>
</web-app>